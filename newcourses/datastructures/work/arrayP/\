#include <stdio.h>
#include <stdlib.h>


struct pair{

    int first;
    int second;
    int third;
};

struct pair add(struct pair p1, struct pair p2)
{
    struct pair temp;
    temp.first = p1.first + p2.first;
    temp.second = p1.second + p2.second;
    return temp;

};

struct pair multiply(struct pair p1, struct pair p2)
{
    struct pair temp;
    temp.first = p1.first * p2.first;
    temp.second = p1.second * p2.second;
    return temp;



}; 
struct multi2(struct pair p1)
{
    struct pair temp;
    temp.first = p1.first * p2.


};
int main(){
    struct pair nu1,nu2,nu3,nu4,nu5,nu6;
    printf("Enter first pair\n");
    scanf("%d %d",&nu1.first, &nu1.second);
    printf("Enter second pair\n");
    scanf("%d %d",&nu2.first, &nu2.second);
    printf("Enter a third number\n");
    scanf("%d",&nu3.third);
    nu4 = add(nu1, nu2);
    printf("Addition result = (%d,%d)\n",nu4.first,nu4.second);
    nu5 = multiply(nu1,nu2);
    printf("Multiplication result of first pair (%d,%d)= (%d,%d)\n",nu1.first,nu1.second,nu5.first,nu5.second);
    nu6= multi2(nu1,nu3);
    printf("C*(A*B) = (%d,%d)",nu6.first,nu6.second);


    return 0;

}
